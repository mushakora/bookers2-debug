<div class="container">
  <p id="notice"></p>
  <div class="row">

<h2>Users search for</h2>



      <table class="table table-striped">
	    <thead>
		  <tr>
			<th>image</th>
			<th>name</th>
			<th colspan="3"></th>
		  </tr>
	    </thead>

	    <body>
	  	<% @users.each do |user| %>
	  	  <tr>
	  	    <td><%= attachment_image_tag(user, :profile_image, :fill, 50, 50, fallback: "no-image-mini.jpg") %></td>
	  	    <td><%= user.name %></td>
	  	    <td><%= link_to 'Show', user_path(user.id) %></td>
	  	  </tr>
	  	<% end %>
	    </body>
      </table>
    </div>
  </div>
</div>